          <th>Products</th>
          <td></td>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><button class="btn btn-xs btn-primary-outline">+</button></td>
          <td>Sample Vendor 1</td>
          <td>1 of 1<%= image_tag('checkmark.svg') %></td>
          <td>0 of 1</td>
          <td>1</td>
