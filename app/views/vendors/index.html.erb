<div class="vendors col-sm-12">
  <ol class="breadcrumb row">
    <li class="active">Dashboard</li>
  </ol>

  <button class="btn btn-primary pull-right">View All Patients</button>

  <section>
  	<table class="table table-sm">
      <thead>
        <tr>
          <td></td>
          <th>Vendor</th>
          <th>Passing Products<%= image_tag('checkmark.svg') %></th>
